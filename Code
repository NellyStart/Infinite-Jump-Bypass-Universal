local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local userInputService = game:GetService("UserInputService")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –ø–ª–∞—Å—Ç–∏–Ω—ã
local function createPlate()
    -- –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—É—é —á–∞—Å—Ç—å (–ø–ª–∞—Å—Ç–∏–Ω—É)
    local plate = Instance.new("Part")
    plate.Name = "JumpPlate"
    plate.Size = Vector3.new(1, 0.1, 1) -- –†–∞–∑–º–µ—Ä –ø–ª–∞—Å—Ç–∏–Ω—ã (—à–∏—Ä–∏–Ω–∞, –≤—ã—Å–æ—Ç–∞, –≥–ª—É–±–∏–Ω–∞)
    plate.Anchored = true -- –§–∏–∫—Å–∏—Ä—É–µ–º –ø–ª–∞—Å—Ç–∏–Ω—É, —á—Ç–æ–±—ã –æ–Ω–∞ –Ω–µ –ø–∞–¥–∞–ª–∞
    plate.CanCollide = true -- –†–∞–∑—Ä–µ—à–∞–µ–º –∫–æ–ª–ª–∏–∑–∏–∏
    plate.Transparency = 1 -- –ü–æ–ª—É–ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –¥–ª—è –≤–∏–∑—É–∞–ª—å–Ω–æ–≥–æ —ç—Ñ—Ñ–µ–∫—Ç–∞
    plate.BrickColor = BrickColor.new("Bright blue") -- –¶–≤–µ—Ç –ø–ª–∞—Å—Ç–∏–Ω—ã

    -- –ü–æ–∑–∏—Ü–∏–æ–Ω–∏—Ä—É–µ–º –ø–ª–∞—Å—Ç–∏–Ω—É –ø–æ–¥ –Ω–æ–≥–∞–º–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if rootPart then
        plate.Position = rootPart.Position - Vector3.new(0, 3, 0) -- –ü–æ–∑–∏—Ü–∏—è –ø–æ–¥ –Ω–æ–≥–∞–º–∏
    end

    -- –î–æ–±–∞–≤–ª—è–µ–º –ø–ª–∞—Å—Ç–∏–Ω—É –≤ –∏–≥—Ä–æ–≤–æ–π –º–∏—Ä
    plate.Parent = workspace

    -- –£–¥–∞–ª—è–µ–º –ø–ª–∞—Å—Ç–∏–Ω—É —á–µ—Ä–µ–∑ 1 —Å–µ–∫—É–Ω–¥—É
    task.delay(0.1, function()
        plate:Destroy()
    end)

    -- –ó–∞—Å—Ç–∞–≤–ª—è–µ–º –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –ø—Ä—ã–≥–Ω—É—Ç—å —Å —Ç–µ–∫—É—â–∏–º –∑–Ω–∞—á–µ–Ω–∏–µ–º JumpPower
    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
end

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è –∑–∞–∂–∞—Ç–∏—è –∫–ª–∞–≤–∏—à–∏
local isSpaceHeld = false

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏—è –∫–ª–∞–≤–∏—à–∏ Space
userInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end -- –ò–≥–Ω–æ—Ä–∏—Ä—É–µ–º, –µ—Å–ª–∏ —Å–æ–±—ã—Ç–∏–µ —É–∂–µ –æ–±—Ä–∞–±–æ—Ç–∞–Ω–æ –∏–≥—Ä–æ–π
    if input.KeyCode == Enum.KeyCode.Space then -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –Ω–∞–∂–∞—Ç–∞ –∫–ª–∞–≤–∏—à–∞ Space
        isSpaceHeld = true
        -- –ó–∞–ø—É—Å–∫–∞–µ–º —Ü–∏–∫–ª —Å–æ–∑–¥–∞–Ω–∏—è –ø–ª–∞—Å—Ç–∏–Ω
        while isSpaceHeld do
            createPlate()
            wait(0.3) -- –ñ–¥–µ–º 1 —Å–µ–∫—É–Ω–¥—É –ø–µ—Ä–µ–¥ —Å–æ–∑–¥–∞–Ω–∏–µ–º —Å–ª–µ–¥—É—é—â–µ–π –ø–ª–∞—Å—Ç–∏–Ω—ã
        end
    end
end)

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—Ç–ø—É—Å–∫–∞–Ω–∏—è –∫–ª–∞–≤–∏—à–∏ Space
userInputService.InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.Space then -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –æ—Ç–ø—É—â–µ–Ω–∞ –∫–ª–∞–≤–∏—à–∞ Space
        isSpaceHeld = false -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ü–∏–∫–ª
    end
end)

--üé©Send Notificationüé©
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

-- –°–æ–∑–¥–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –æ–ø–æ–≤–µ—â–µ–Ω–∏—è
local function sendNotification()
    local message = "github.com/NellyStart"
    
    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –≤—Å–µ–º –∏–≥—Ä–æ–∫–∞–º
    StarterGui:SetCore("SendNotification", {
        Title = "Inf Jump Bypass",
        Text = message,
        Duration = 6 -- –ü—Ä–æ–¥–æ–ª–∂–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è –≤ —Å–µ–∫—É–Ω–¥–∞—Ö
    })
end

-- –í—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é
sendNotification()
